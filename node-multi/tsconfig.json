{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "baseUrl": "./",
    "paths": {
      "~models/*": ["models/*"],
      "~web/*": ["web/*"],
      "~worker/*": ["worker/*"]
    },
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  },
    "exclude": [
      "node_modules"
    ]
}
